package alg;

import java.util.HashSet;

public class TestClient {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static int[][] buildArray(String input, Integer out) {
		HashSet<Character> tiles;
		
		String[] rows = input.split("\n");
		int[][] resultArray = new int[rows[0].length()][rows.length];
		
		for(int i = 0; i < rows[0].length(); i++) {
			for(int j = 0; j < rows.length; j++) {
				if(rows[j].length() != rows[0].length())
					throw new Exception("All rows in input string need to be same length");
				
				resultArray[i][j] = rows[j].charAt(i);
			}
		}
	}
}
